#include <stdio.h>
void main()
{
 int i,j,matrix[3][3];
 printf("\n Enter the Matrix Elements=");
 for(i=0;i<3;i++)
 {
  for(j=0;j<3;j++)  
  {
   scanf("%d \n",&matrix[i][j]);
  } 
 }
 printf(" Array Elements in Matrix=\n");
 for(i=0;i<3;i++)
 {
  for(j=0;j<3;j++)  
  {
   printf("%d    ",matrix[i][j]);
  } 
  printf("\n");
 }
}
