#include <stdio.h>
void main()
{
    int size=10,no,i,flag=0,array[size];
    printf("Enter the Elements=\n");
    for(i=0;i<size;i++)
    {
        scanf("%d",&array[i]);
    }
    printf("\n Enter the element you want to search=");
    scanf("%d",&no);
    for(i=0;i<size;i++)
    {if(no==array[i])
    {
        flag=1;
        break;
    }
    }
    if(flag==1)
    {
        printf("\n Search is successful at index=%d",i);
    }
    else
    {
        printf("\n Search is unsuccessful");
    }
}
